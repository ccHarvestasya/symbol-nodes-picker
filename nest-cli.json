{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      { "include": "./config/*.yaml", "outDir": "./dist", "watchAssets": true },
      {
        "include": "util/symboler/cert/*.pem",
        "outDir": "./dist",
        "watchAssets": true
      }
    ]
  }
}
